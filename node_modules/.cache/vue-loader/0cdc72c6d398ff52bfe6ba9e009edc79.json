an class=\"s1\">作者:</span>\n      <input type=\"text\" v-model=\"myObj.author\">\n    </div>\n\n    <div class=\"class\">\n      <span class=\"s2\">分类:</span>\n      <select class=\"cls\" v-model=\"myObj.class_id\">\n        <option v-for=\"item in classList\" :key=\"item.class_id\" :value=\"item.class_id\">{{item.class_content}}</option>\n      </select>\n    </div>\n\n    <div class=\"content\">\n      <span class=\"s4\">博客内容:</span>\n      <textarea v-model=\"myObj.content\"></textarea>\n    </div>\n    <button class=\"commit\" @click=\"onSubmit\">发表博客</button>\n    <hr>\n    <div v-show=\"myObj.title || myObj.author || myObj.content\">\n      <h1>博客预览</h1>\n      <bolgItem :itemObj=\"myObj\"/>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport bolgItem from './BlogItem'\nimport request from '../utils/request.ts'\n\nexport default {\n  data () {\n    return {\n      myObj: {\n        title: '',\n        author: '',\n        content: '',\n        class_id: 0,\n        created_at: new Date().toLocaleString()\n      },\n      classList: []\n    }\n  },\n  components: {\n    bolgItem\n  },\n  created () {\n    this.getClass()\n  },\n  methods: {\n    // 获取类型列表\n    getClass () {\n      request({\n        url: 'http://127.0.0.1/class'\n      }).then(res => {\n        this.classList = res.data.data\n        this.myObj.class_id = this.classList[0].class_id\n      })\n    },\n    // 提交\n    onSubmit () {\n      if (this.myObj.class_id == 0 ||\n          this.myObj.title == '' ||\n          this.myObj.author == '' ||\n          this.myObj.content == '') {\n        alert('缺少必填项')\n        return\n      }\n      this.pushData()\n      alert('发表成功')\n      this.$router.push('/')\n    },\n    pushData () {\n      request({\n        url: 'http://127.0.0.1/add',\n        params: this.myObj\n      }).then(res => {\n        console.log(res)\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n  .add {\n    padding-top: 50px;\n    padding-bottom: 350px;\n  }\n\n  .title, .author, .class {\n    padding-top: 10px;\n    padding-right: 30px;\n    padding-left: 30px;\n  }\n\n  .content {\n    display: flex;\n    padding-top: 10px;\n\n    .s4 {\n      padding-top: 10px;\n      padding-left: 130px;\n    }\n  }\n\n  input, select {\n    outline-style: none;\n    border: 1px solid #ccc;\n    border-radius: 3px;\n    margin: 10px;\n    width: 620px;\n    height: 30px;\n    font-size: 14px;\n    font-weight: 700;\n    font-family: \"Microsoft soft\";\n  }\n\n  .cls {\n    width: 310px;\n  }\n\n  .s3 {\n    padding-left: 100px;\n  }\n\n  .s1, .s2 {\n    padding-left: 132px;\n  }\n\n  .commit {\n    margin-left: 206px;\n    margin-top: 20px;\n    margin-bottom: 40px;\n    border: 1px solid #ccc;\n    border-radius: 3px;\n    width: 230px;\n    height: 30px;\n    background: #41B883;\n    color: white;\n  }\n\n  textarea {\n    outline-style: none;\n    border: 1px solid #ccc;\n    border-radius: 3px;\n    margin: 10px;\n    width: 620px;\n    height: 120px;\n    font-size: 14px;\n    font-weight: 700;\n    font-family: \"Microsoft soft\";\n  }\n\n  h1 {\n    font-size: 22px;\n    color: #555;\n    padding: 50px 0 30px;\n    text-align: center;\n    border-top: 1px solid white;\n    margin-top: 30px;\n  }\n\n  hr {\n    background-color: rgba(0, 0, 0, 0.1);\n    height: 1px;\n    border: none;\n    width: 95%;\n    align-items: center;\n    margin-left: 25px;\n  }\n</style>\n"]}]}