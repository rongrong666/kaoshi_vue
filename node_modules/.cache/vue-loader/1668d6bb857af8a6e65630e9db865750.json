e = new Date(parseInt(this.qyear), parseInt(this.qmonth) - 1, parseInt(this.qday),\n          parseInt(this.qhour) + hour, parseInt(this.qmin) + min)\n        const lefttime = endtime.getTime() - nowtime.getTime()\n        const lefth = Math.floor(lefttime / (1000 * 60 * 60) % 24)\n        const leftm = Math.floor(lefttime / (1000 * 60) % 60)\n        const lefts = Math.floor(lefttime / 1000 % 60)\n        this.time = '剩余时间：' + lefth + ':' + leftm + ':' + lefts\n        if (nowtime >= endtime) {\n          this.sub2()\n          this.time = '00:00:00'\n          this.isDisable = true\n          clearInterval(this.gettime)\n          this.$router.replace('/wait3')\n        }\n      },\n      sub12 () {\n        if (!this.a1 ||\n          !this.a2 ||\n          !this.a3 ||\n          !this.a4 ||\n          !this.a5 ||\n          !this.a6 ||\n          !this.a7 ||\n          !this.a8 ||\n          !this.a9 ||\n          !this.a10 ||\n          !this.a11 ||\n          !this.a12 ||\n          !this.a13 ||\n          !this.a14 ||\n          !this.a15 ||\n          !this.a16 ||\n          !this.a17 ||\n          !this.a18 ||\n          !this.a19 ||\n          !this.a20 ||\n          !this.a21 ||\n          !this.a22 ||\n          !this.a23 ||\n          !this.a24 ||\n          !this.a25 ||\n          !this.a26 ||\n          !this.a27 ||\n          !this.a28 ||\n          !this.a29 ||\n          !this.a30 ||\n          !this.a31 ||\n          !this.a32 ||\n          !this.a33 ||\n          !this.a34 ||\n          !this.a35 ||\n          !this.a36 ||\n          !this.a37 ||\n          !this.a38 ||\n          !this.a39 ||\n          !this.a40 ||\n          !this.b1 ||\n          !this.b2 ||\n          !this.b3 ||\n          !this.b4 ||\n          !this.b5 ||\n          !this.b6 ||\n          !this.b7 ||\n          !this.b8 ||\n          !this.b9 ||\n          !this.b10) {\n          this.$message.error('请完成所有选择题！(包括判断、单选、多选)')\n          return\n        }\n        sub2(\n    