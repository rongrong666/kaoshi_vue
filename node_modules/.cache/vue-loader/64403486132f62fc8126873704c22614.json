: '',\n        qhour: '',\n        qmin: '',\n        content: '',\n        content1: '',\n        tp: 0,\n        radio: '0',\n        radio1: '0',\n        radio2: '0',\n        radio3: '0',\n        radio4: '0',\n        radio5: '0',\n        radio6: '0',\n        radio7: '0',\n        radio8: '0',\n        radio9: '0',\n        radio10: '0',\n        radio11: '0'\n      };\n    },\n    created () {\n      this.tp = localStorage.getItem('userType')\n      if (this.tp == 1) {\n        // todo:试题选择\n        this.content1 = '会计试题'\n        this.isShow1 = true\n      } else {\n        this.content1 = '文秘试题'\n        this.isShow2 = true\n      }\n      request({\n        method: 'get',\n        url: 'http://49.232.20.84:8080/user/setting'\n      }).then(res => {\n        if (res.data.code === 20000) {\n          this.qyear = res.data.data.Qyear\n          this.qmonth = res.data.data.Qmonth\n          this.qday = res.data.data.Qday\n          this.qhour = res.data.data.Qhour\n          this.qmin = res.data.data.Qmin\n        } else {\n          this.$message({\n            message: '未获取到考试时间!',\n            type: 'warning'\n          })\n        }\n      })\n      setInterval(this.gettime, 1000)\n    },\n    methods: {\n      gettime () {\n        const nowtime = new Date()\n        const endtime = new Date(parseInt(this.qyear), parseInt(this.qmonth) - 1, parseInt(this.qday),\n                            parseInt(this.qhour) + 2, parseInt(this.qmin))\n        const lefttime = endtime.getTime() - nowtime.getTime()\n        const lefth = Math.floor(lefttime / (1000 * 60 * 60) % 24)\n        const leftm = Math.floor(lefttime / (1000 * 60) % 60)\n        const lefts = Math.floor(lefttime / 1000 % 60)\n        this.content = '剩余时间：' + lefth + ':' + leftm + ':' + lefts\n        if (nowtime >= endtime) {\n          this.content = '00:00:00'\n          this.isDisable = true\n          clearInterval(this.gettime)\n          this.$router.push('/wait2')\n        }\n      },\n      sub () {\n        if (this.radio === '0' ||\n            this.radio1 === '0' ||\n            this.radio2 === '0' ||\n            this.radio3 === '0' ||\n            this.radio4 === '0' ||\n            this.radio5 === '0' ||\n            this.radio6 === '0' ||\n            this.radio7 === '0' ||\n            this.radio8 === '0' ||\n            this.radio9 === '0' ||\n            this.radio10 === '0') {\n          this.$message({\n            message: '请完成所有题目!',\n            type: 'warning'\n          })\n        } else {\n          // todo:计算出成绩\n          const token = localStorage.getItem('token')\n          request({\n            method: 'post',\n            url: 'http://49.232.20.84:8080/user/sub?token=' + token.substring(1, token.length - 1),\n            data: {\n              a1: {\n                key1: this.radio,\n                key2: this.radio1,\n                key3: this.radio2,\n                key4: this.radio3,\n                key5: this.radio4,\n                key6: this.radio5\n              },\n              b1: {\n                key7: this.radio6,\n                key8: this.radio7,\n                key9: this.radio8,\n                key10: this.radio9,\n                key11: this.radio10\n              }\n            }\n          }).then(res => {\n              console.log(res.data.code)\n            }\n          )\n          this.$router.replace('/wait2')\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"less\" scoped>\n  .wait_ma{\n    background-color: white;\n    padding: 20px;\n    height: 1000px;\n    width: auto;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .test{\n    background-color: #dedcdb;\n    height: auto;\n    width: 100%;\n  }\n  .test1{\n    background-color: #dedcdb;\n    height: auto;\n    width: 100%;\n  }\n  .question{\n    background-color: white;\n    height: auto;\n    width: 80%;\n  }\n  .el-col {\n    border-radius: 4px;\n  }\n  .bg-purple-dark {\n    background