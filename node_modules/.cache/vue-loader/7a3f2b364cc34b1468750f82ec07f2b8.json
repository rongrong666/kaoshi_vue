  <div>\n          <div>验证码</div>\n          <el-input  v-model=\"code\" placeholder=\"请输入\"></el-input>\n          <el-button id='bt' style=\"margin-top: 10px\" @click=\"queryMessage\">点击发送验证码</el-button>\n        </div>\n        <el-button id=\"loginL\" @click=\"loginLogin\">登录</el-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import request from '../utils/request.ts'\n  import store from '../../store/index'\n  export default {\n    data () {\n      return {\n        username: '',\n        mobile: '',\n        uid: '',\n        code: ''\n      }\n    },\n    methods: {\n      queryMessage () {\n        request({\n          method: 'post',\n          url: 'http://127.0.0.1:8080/login/message',\n          data: {\n            username: this.username,\n            mobile: this.mobile,\n            uid: this.uid\n          }\n        }).then(res => {\n          if (res.data.code === 20000) {\n            alert('短信发送成功')\n          } else {\n            alert('请填写正确身份信息')\n          }\n          }\n        )\n      },\n      loginLogin () {\n        request({\n          method: 'post',\n          url: 'http://127.0.0.1:8080/login/login',\n          data: {\n            username: this.username,\n            mobile: this.mobile,\n            uid: this.uid,\n            code: this.code\n          }\n        }).then(res => {\n          if (res.data.code === 20000) {\n            store.commit('setToken', res.data.data.token)\n            alert('短信发送成功')\n          } else {\n            alert('请填写正确身份信息')\n          }\n          }\n        )\n      }\n    }\n  }\n</script>\n\n<style lang=\"less\" scoped>\n  .ma{\n    background-image:url('../../public/test.png');\n    padding: 20px;\n    height: 1000px;\n    width: auto;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .bd{\n    height: 500px;\n    width: 500px;\n    background: white;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    border-radius: 6px;\n  }\n\n  #loginL {\n    width: 100%;\n    background-color: #2d8eff;\n    color: #dedcdb;\n    padding: 14px 20px;\n    margin: 8px 0;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n  }\n\n  #loginL:hover {\n    background-color: #1f27ff;\n  }\n  div {\n    color: #6a6d73;\n  }\n</style>\n"]}]}