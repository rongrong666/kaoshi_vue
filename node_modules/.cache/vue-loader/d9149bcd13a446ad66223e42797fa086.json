omponents: {\n    bolgItem\n  },\n  created () {\n    // 获取网络数据\n    this.getData()\n  },\n  methods: {\n    btnLeft () {\n      this.page--\n      if (this.page < 0) {\n        alert('已经是第一页了')\n        this.page++\n        return\n      }\n      this.getData()\n      this.scrTop()\n    },\n    btnRit () {\n      this.page++\n      if (this.listArr == null) {\n        this.page--\n        alert('已经没有更多')\n        return\n      }\n      if (this.listArr.length < 8) {\n        this.page--\n        alert('已经没有更多')\n        return\n      }\n      this.getData()\n      this.scrTop()\n    },\n    // 翻页回到顶部\n    getData () {\n      request({\n        method: 'get',\n        url: 'http://127.0.0.1:8000/select',\n        params: {\n          page: this.page\n        }\n      }).then(res => {\n        this.listArr = res.data.data\n      }\n      )\n    },\n\n    scrTop () {\n      if (document.body.scrollTop) {\n        document.body.scrollTop = 0\n      } else {\n        document.documentElement.scrollTop = 0\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n  .show {\n    padding: 30px;\n  }\n\n  .pageGroup {\n    padding: 40px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    .btn {\n      border: 1px solid #41B883;\n      padding: 10px 20px;\n      color: #41B883;\n      font-size: 18px;\n      margin: 0 10px;\n      cursor: pointer;\n\n      &:hover {\n        background: #41B883;\n        color: white;\n      }\n    }\n  }\n\n</style>\n"]}]}